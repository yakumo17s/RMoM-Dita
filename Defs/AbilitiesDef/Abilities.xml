    <TorannMagic.TMAbilityDef ParentName="BaseMagicAbility">
        <defName>Dita_Data_Backup</defName>
        <label>Data_Backup</label>
        <description>Q skill</description>
        <uiIconPath>Dita_Abilities/Dita_P_Data_Backup</uiIconPath>
